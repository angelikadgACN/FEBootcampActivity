<mvc:View
    controllerName="febootcamp.controller.View"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core" 
    xmlns:viz="sap.viz.ui5.controls" 
    xmlns:layout="sap.ui.layout" 
    xmlns:viz.feeds="sap.viz.ui5.controls.common.feeds" 
    xmlns:viz.data="sap.viz.ui5.data" height="100%">
    <!-- <Page id="page" title="{i18n>title}">
            		 <Button id="_IDGenButton" text="Hello, Angelika" press="onPress" />   
                </Page>-->
    <Panel id="_IDGenPanel" width="auto" class="sapUiResponsiveMargin">
        <headerToolbar>
            <OverflowToolbar id="_IDGenOverflowToolbar1">
                <Title id="_IDGenTitle" text="Hello, Angelika"/>
                <Button id="_IDGenButton" icon="sap-icon://comment" text="Click to say Hello!" press="onHello"/>
            </OverflowToolbar>
        </headerToolbar>
        <content>
            <viz:VizFrame id="idVizFrame" uiConfig="{applicationSet:'fiori'}"
                height='100%' width="100%" vizType='column'>
                <viz:dataset>
                    <viz.data:FlattenedDataset id="_IDGenFlattenedDataset1" data="{/createdData}">
                        <viz.data:dimensions>
                            <viz.data:DimensionDefinition id="_IDGenDimensionDefinition" name="Month"
                                value="{month}" />
                        </viz.data:dimensions>
                        <viz.data:measures>
                            <viz.data:MeasureDefinition id="_IDGenMeasureDefinition" name="Created Data"
                                value="{created}" />
                        </viz.data:measures>
                    </viz.data:FlattenedDataset>
                </viz:dataset>

                <viz:feeds>
                    <viz.feeds:FeedItem id='valueAxisFeed' uid="valueAxis" type="Measure"
                        values="created" />
                    <viz.feeds:FeedItem id='categoryAxisFeed' uid="categoryAxis" type="Dimension"
                        values="month" />
                </viz:feeds>
                <viz:dependents>
                    <viz:Popover id="idPopOver"></viz:Popover>
                </viz:dependents>
            </viz:VizFrame>

            <List id="_IDGenList" headerText="List" items="{/items}">
                <StandardListItem id="_IDGenStandardListItem" title="{name}"/>
            </List>

            <Link id="_IDGenLink" text="Click here to go to Page 2!" press="onNavToPage2"/>

        </content>
    </Panel>
</mvc:View>
